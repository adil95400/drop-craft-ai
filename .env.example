# Environment Configuration for Drop Craft AI
# Copy this file to .env and fill in your actual values

# ================================
# SUPABASE CONFIGURATION
# ================================
VITE_SUPABASE_PROJECT_ID=your_project_id_here
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_PUBLISHABLE_KEY=your_publishable_key_here

# ================================
# E-COMMERCE INTEGRATIONS
# ================================

# Shopify Integration
SHOPIFY_API_KEY=your_shopify_api_key
SHOPIFY_API_SECRET=your_shopify_api_secret
SHOPIFY_WEBHOOK_SECRET=your_shopify_webhook_secret

# AliExpress Integration  
ALIEXPRESS_API_KEY=your_aliexpress_api_key
ALIEXPRESS_SECRET_KEY=your_aliexpress_secret_key
ALIEXPRESS_SESSION_KEY=your_aliexpress_session_key

# BigBuy Integration
BIGBUY_API_KEY=your_bigbuy_api_key
BIGBUY_SECRET=your_bigbuy_secret

# Amazon Integration
AMAZON_ACCESS_KEY_ID=your_amazon_access_key
AMAZON_SECRET_ACCESS_KEY=your_amazon_secret_key
AMAZON_MARKETPLACE_ID=your_amazon_marketplace_id

# eBay Integration
EBAY_CLIENT_ID=your_ebay_client_id
EBAY_CLIENT_SECRET=your_ebay_client_secret
EBAY_REDIRECT_URI=your_ebay_redirect_uri

# WooCommerce Integration
WOOCOMMERCE_CONSUMER_KEY=your_woocommerce_consumer_key
WOOCOMMERCE_CONSUMER_SECRET=your_woocommerce_consumer_secret

# ================================
# MARKETING & ANALYTICS
# ================================

# Google Integration
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_ADS_CLIENT_ID=your_google_ads_client_id
GOOGLE_ANALYTICS_TRACKING_ID=your_ga_tracking_id

# Facebook Integration
FACEBOOK_APP_ID=your_facebook_app_id
FACEBOOK_APP_SECRET=your_facebook_app_secret
FACEBOOK_ACCESS_TOKEN=your_facebook_access_token

# Mailchimp Integration
MAILCHIMP_API_KEY=your_mailchimp_api_key
MAILCHIMP_SERVER_PREFIX=your_mailchimp_server_prefix

# Klaviyo Integration
KLAVIYO_PUBLIC_API_KEY=your_klaviyo_public_key
KLAVIYO_PRIVATE_API_KEY=your_klaviyo_private_key

# ================================
# REVIEWS & CUSTOMER SERVICE
# ================================

# Loox Integration
LOOX_API_KEY=your_loox_api_key
LOOX_SHOP_ID=your_loox_shop_id

# Judge.me Integration
JUDGE_ME_API_TOKEN=your_judge_me_api_token
JUDGE_ME_SHOP_DOMAIN=your_judge_me_shop_domain

# Trustpilot Integration
TRUSTPILOT_API_KEY=your_trustpilot_api_key
TRUSTPILOT_BUSINESS_UNIT_ID=your_trustpilot_business_unit_id

# Zendesk Integration
ZENDESK_SUBDOMAIN=your_zendesk_subdomain
ZENDESK_EMAIL=your_zendesk_email
ZENDESK_API_TOKEN=your_zendesk_api_token

# ================================
# PAYMENT PROCESSING
# ================================

# Stripe Integration
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
STRIPE_WEBHOOK_SECRET=whsec_your_stripe_webhook_secret

# PayPal Integration
PAYPAL_CLIENT_ID=your_paypal_client_id
PAYPAL_CLIENT_SECRET=your_paypal_client_secret
PAYPAL_MODE=sandbox # or live

# Klarna Integration
KLARNA_USERNAME=your_klarna_username
KLARNA_PASSWORD=your_klarna_password
KLARNA_BASE_URL=https://api.playground.klarna.com # or https://api.klarna.com for live

# ================================
# SHIPPING & TRACKING
# ================================

# 17track Integration
TRACK17_API_KEY=your_17track_api_key

# AfterShip Integration
AFTERSHIP_API_KEY=your_aftership_api_key

# ================================
# AI & CONTENT GENERATION
# ================================

# OpenAI Integration
OPENAI_API_KEY=your_openai_api_key
OPENAI_ORGANIZATION_ID=your_openai_org_id

# Anthropic Claude (Alternative)
ANTHROPIC_API_KEY=your_anthropic_api_key

# ================================
# DEVELOPMENT & DEBUGGING
# ================================

# Environment
NODE_ENV=development
VITE_APP_ENV=development

# Debug flags
VITE_ENABLE_RQ_DEVTOOLS=true
VITE_ENABLE_DEBUG_LOGS=true
VITE_ENABLE_PERFORMANCE_MONITORING=false

# ================================
# SECURITY & MONITORING
# ================================

# Encryption keys (generate strong random keys)
SUPPLIER_ENCRYPTION_KEY=your_32_character_encryption_key_here
JWT_SECRET=your_jwt_secret_key_here

# Rate limiting
VITE_RATE_LIMIT_REQUESTS_PER_MINUTE=100

# CORS origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# ================================
# OPTIONAL INTEGRATIONS
# ================================

# Sentry Error Tracking
SENTRY_DSN=your_sentry_dsn
SENTRY_AUTH_TOKEN=your_sentry_auth_token

# Hotjar Analytics
HOTJAR_ID=your_hotjar_id

# Intercom Customer Support
INTERCOM_APP_ID=your_intercom_app_id

# ================================
# BACKUP & STORAGE
# ================================

# AWS S3 (for file storage)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=your_aws_region
AWS_S3_BUCKET=your_s3_bucket_name

# ================================
# NOTES
# ================================
# 
# 1. Never commit this file with real values to version control
# 2. Keep your API keys secure and rotate them regularly
# 3. Use different keys for development, staging, and production
# 4. Some integrations may require additional configuration
# 5. Check each service's documentation for specific setup instructions
#
# For production deployment, set these variables in your hosting platform:
# - Vercel: Project Settings > Environment Variables
# - Netlify: Site Settings > Build & Deploy > Environment Variables
# - Railway: Project Settings > Variables
# - Heroku: Settings > Config Vars