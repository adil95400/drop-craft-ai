# Facebook Ads - Publicités Facebook/Instagram
FACEBOOK_ACCESS_TOKEN=votre_token_facebook_ads
FACEBOOK_APP_SECRET=votre_app_secret_facebook

# Avis et reviews
# ==============

# Loox - Gestion d'avis produits
LOOX_API_KEY=votre_cle_api_loox

# Judge.me - Plateforme d'avis
JUDGE_ME_API_TOKEN=votre_token_judge_me

# IA et optimisation
# =================

# OpenAI - Génération de contenu et optimisation
OPENAI_API_KEY=votre_cle_api_openai

